  .modal-confirm .modal-content {
    padding: 20px;
    border-radius: 5px;
    border: none;        
  }
  .modal-confirm .modal-header {
        padding: 0 15px;
    border-bottom: none;   
        position: relative;
  }
  .modal-confirm h4 {
        display: inline-block;
    font-size: 26px;
  }
  .modal-confirm .close {
        position: absolute;
    top: -5px;
    right: -5px;
  }
    .modal-confirm .modal-body {
